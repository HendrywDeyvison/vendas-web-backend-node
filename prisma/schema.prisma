datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id  Int @id @default(autoincrement())
  name  String
  email String @unique
  cpf_cnpj  String @unique
  password String
  phone String
  typeUser  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

generator client {
  provider = "prisma-client-js"
}